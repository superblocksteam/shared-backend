{
  "name": "@superblocksteam/shared-backend",
  "description": "Superblocks Backend Shared Resources",
  "version": "0.2022.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/superblocksteam/shared-backend.git",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^16.14.0",
    "npm": "^8.3.1"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist *tsbuildinfo",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint . --ext ts --fix",
    "typecheck": "tsc",
    "check": "npm run typecheck && npm run lint",
    "test": "jest --coverage"
  },
  "license": "Superblocks Community Software License",
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/lodash": "^4.14.170",
    "@types/node": "^16.7.10",
    "@types/systeminformation": "^3.54.1",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "^4.3.8",
    "jest": "26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "26.5.6",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "@superblocksteam/shared": "^0.x",
    "aws-sdk": "2.1084.0",
    "axios": "0.25.0",
    "bson": "4.6.5",
    "express": "4.18.1",
    "form-data": "^4.0.0",
    "json5": "2.2.1",
    "lodash": "4.17.21",
    "mustache": "4.2.0",
    "pino": "6.11.0",
    "vm2": "3.9.10",
    "xmlbuilder2": "3.0.2",
    "moment": "2.29.4",
    "base64url": "3.0.1",
    "deasync": "0.1.26"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint . --ext ts --cache --fix"
  }
}
